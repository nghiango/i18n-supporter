{
  "name": "i18n-supporter",
  "version": "0.1.1",
  "main": "electron/dist/electron/src/main.js",
  "scripts": {
    "ng": "ng",
    "angular:quickdev": "ng serve --open --port=4200",
    "angular:integrate": "ng serve -c integrate --port=4201",
    "electron:integrate": "export DEVELOPMENT_MODE=true; electron .",
    "angular:build": "node ./build/clean-build.js && ng build --prod -c electron --base-href ./ ",
    "electron:build": "tsc --p electron",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:build-all": "npm run angular:build && npm run electron:build",
    "electron:package": "npm run electron:build-all && electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "flat": "^5.0.0",
    "fs-extra": "^9.0.0",
    "jquery": "^3.4.1",
    "popper": "^1.0.1",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@types/jest": "^26.0.21",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^9.4.0",
    "electron-packager": "^15.2.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
